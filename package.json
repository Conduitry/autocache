{
	"name": "@conduitry/autocache",
	"version": "0.1.6",
	"description": "A cache that cleans itself",
	"keywords": [
		"cache"
	],
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"*.md",
		"dist"
	],
	"engines": {
		"node": ">=12"
	},
	"repository": {
		"type": "git",
		"url": "https://git.chor.date/Conduitry/autocache"
	},
	"author": "Conduitry",
	"license": "MIT",
	"homepage": "https://git.chor.date/Conduitry/autocache",
	"devDependencies": {
		"@types/node": "=12",
		"rollup": "^2",
		"rollup-plugin-cheap-ts": "Conduitry/rollup-plugin-cheap-ts#semver:^1",
		"typescript": "^4"
	},
	"scripts": {
		"build": "rollup -c",
		"dev": "rollup -cw",
		"prepare": "npm run build",
		"pretest": "npm run build",
		"test": "node test.js"
	}
}
